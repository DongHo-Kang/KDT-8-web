      <br />
      <fieldset>
        <legend>관심사</legend>
        <input type="checkbox" value="여행" name="hobby" id="hobby" />여행
        <input type="checkbox" value="패션" name="hobby" id="hobby" />패션
        <input type="checkbox" value="음식" name="hobby" id="hobby" />음식
      </fieldset>
      <br />
      <button type="submit">회원가입</button>
